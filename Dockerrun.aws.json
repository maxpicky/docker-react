{
    "AWSEBDockerrunVersion": 2,
    "volumes": [
    
      {
        "name": "nginx"
        }
    ],
    "containerDefinitions": [
      {
        "name": "nginx",
        "image": "nginx",
        "essential": true,
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ]
      }
    ]
  }